        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BooleanNetworkSupportTool"
        mc:Ignorable="d"
        Title="New Boolean Variable" Height="137.666" Width="366.001">

    <StackPanel>
        <Label Content="Enter name of new variable: " Margin="22,10,0,10" />
        <TextBox x:Name="txtInputVariable" Margin="22,0,104.333,0"></TextBox>
        <Button Content="OK" Click="OKButton_Click" TouchEnter="OKButton_Click" Margin="276,10,12.333,0" RenderTransformOrigin="0.5,0.5" />
    </StackPanel>
</Window>
